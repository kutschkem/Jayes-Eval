<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="Evaluation"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
            <run class="org.eclipse.recommenders.eval.jayes.apps.Evaluation">
            </run>
      </application>
   </extension>
   <extension
         id="Diagrams"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.recommenders.eval.jayes.apps.DiagramGeneratorApp">
         </run>
      </application>
   </extension>
   <extension
         id="StabilityBenchmark"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.recommenders.eval.jayes.apps.StabilityBenchmark">
         </run>
      </application>
   </extension>

</plugin>
